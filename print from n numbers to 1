import java.util.*;
public class Main
{
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int num=sc.nextInt();
	    print(num,1);
	}
	public static void print(int n,int i)
	{
	    if(n<i)
	    {
	        return ;
	    }
	    System.out.println(n);
	    print(n-1,i);
	}
}
